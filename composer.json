{
    "name": "sebbmeyer/laravel-teams-connector",
    "description": "Microsoft Teams Connector for Laravel 5/6",
    "keywords": ["laravel", "teams", "microsoft", "connectors", "connector", "messagecard"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Meyer",
            "email": "sebbmyr@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.5.x|5.6.x|5.7.x|5.8.x|6.x|7.x",
        "sebbmeyer/php-microsoft-teams-connector": "0.1.*"
    },
    "autoload": {
        "psr-4": {
            "Sebbmyr\\LaravelTeams\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sebbmyr\\LaravelTeams\\TeamsConnectorServiceProvider"
            ],
            "aliases": {
                "TeamsConnector": "Sebbmyr\\LaravelTeams\\Facades\\TeamsConnector"
            }
        }
    },
    "minimum-stability": "dev"
}
